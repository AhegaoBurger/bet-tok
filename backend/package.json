    "hono": "^4.6.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@vitest/coverage-v8": "^4.0.16",
    "msw": "^2.12.7",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0",
    "vitest": "^4.0.16"
  }
}
